@keyframes m-move
{
0% {
  -webkit-transform: rotate(-5deg);
  transform: rotate(-5deg);
}
50% {
  -webkit-transform: rotate(0deg);
  transform: rotate(0deg);
}
100% {
  -webkit-transform: rotate(5deg);
  transform: rotate(5deg);
}
}

@-webkit-keyframes m-move /* Safari and Chrome */
{
  0% {
    -webkit-transform: rotate(-5deg);
    transform: rotate(-5deg);
  }
  50% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(5deg);
    transform: rotate(5deg);
  }
}
